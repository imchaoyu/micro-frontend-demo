{
  "name": "micro-frontend-demo",
  "version": "0.1.0",
  "description": "a Micro-Frontend project demo based on qiankun ",
  "main": "index.js",
  "scripts": {
    "pro:install": "npm-run-all --serial install:*",
    "pro:start": "npm-run-all --parallel start:*",
    "install:base": "cd src/base && yarn",
    "start:base": "cd src/base && yarn start",
    "install:react": "cd src/app-react && yarn",
    "start:react": "cd src/app-react && yarn start",
    "install:vue": "cd src/app-vue && yarn",
    "start:vue": "cd src/app-vue && yarn serve"
  },
  "keywords": [
    "qiankun",
    "micro-frontend"
  ],
  "author": "chaoyu",
  "license": "WTFPL",
  "dependencies": {
    "typescript": "^4.1.2"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
